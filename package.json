    "release": "turbo run build --filter=@repo/ui && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.9",
    "turbo": "^2.0.12",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  }
}
