{
  "name": "setaday",
  "private": true,
  "scripts": {
    "test": "turbo test",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:css": "turbo run build:css",
